<Procedure> <Name> BalançageFouettageGauche </Name>  
	<Sprite><Name> Schizy </Name><Move><X>4</X><Y>-2</Y></Move></Sprite>
	<Sprite><Name> Menottes </Name><Move><X>3</X><Y>-2</Y></Move></Sprite>
