	<Sprite><Name> Chaine_2 </Name><Move><X>2</X><Y>-2</Y></Move></Sprite>
	<Time>240</Time>
	<Sprite><Name> Schizy </Name><Move><X>-7</X><Y>1</Y></Move></Sprite>
	<Sprite><Name> Menottes </Name><Move><X>-5</X><Y>1</Y></Move></Sprite>
	<Sprite><Name> Chaine_1 </Name><Move><X>-3</X><Y>1</Y></Move></Sprite>
	<Sprite><Name> Chaine_2 </Name><Move><X>-3</X><Y>1</Y></Move></Sprite>
	<Time>160</Time>
	<Sprite><Name> Schizy </Name><Move><X>5</X><Y>1</Y></Move></Sprite>
	<Sprite><Name> Menottes </Name><Move><X>3</X><Y>1</Y></Move></Sprite>
	<Sprite><Name> Chaine_1 </Name><Move><X>1</X><Y>1</Y></Move></Sprite>
	<Sprite><Name> Chaine_2 </Name><Move><X>1</X><Y>1</Y></Move></Sprite>
	<Time>160</Time>
	<Sprite><Name> Schizy </Name><Move><X>-2</X><Y>0</Y></Move></Sprite>
